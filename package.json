{
  "name": "portfolio-agents",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsx src/index.ts",
    "run:weekly": "cross-env RUN=weekly tsx src/index.ts",
    "run:monthly": "cross-env RUN=monthly tsx src/index.ts",
    "worker:dev": "wrangler dev worker/index.ts",
    "worker:deploy": "wrangler deploy worker/index.ts",
    "report:cache": "tsx scripts/cache-series.ts",
    "report:html": "tsx scripts/report.ts",
    "weekly:all": "npm run run:weekly && npm run report:cache && npm run report:html && npm run report:dashboard",
    "test": "vitest run",
    "report:dashboard": "tsx scripts/dashboard.ts",
    "av:discover": "tsx scripts/alpha-discover.ts",
    "check:data": "tsx scripts/check-data.ts",
    "sync:data": "tsx scripts/sync-data.ts"
  },
  "dependencies": {
    "dotenv": "^16.6.1",
    "node-fetch": "^3.3.2",
    "yaml": "^2.8.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "js-yaml": "^4.1.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.9"
  }
}
